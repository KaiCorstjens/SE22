<configuration>
  <system.web>
    <customErrors mode="Off"/>
    <compilation targetFramework="4.5" />
    <httpRuntime targetFramework="4.5" 
                 executionTimeout="90"
                 maxRequestLength="8000"/>
  </system.web>
<system.webServer>
    <security>
      <requestFiltering allowDoubleEscaping="true" /><!-- Yay for opening security holes-->
    </security>
  </system.webServer>
</configuration>